#!/usr/bin/env bash

set -euo pipefail

GH_REPO="https://github.com/arstella-ltd/RedmineCLI"
TOOL_NAME="redmine"

plugin_dir=$(dirname "$(dirname "${BASH_SOURCE[0]}")")

# shellcheck source=../lib/utils.bash
source "${plugin_dir}/lib/utils.bash"

# Get the latest stable version (exclude pre-releases)
list_all_versions | grep -E '^[0-9]+\.[0-9]+\.[0-9]+$' | sort_versions | tail -n1